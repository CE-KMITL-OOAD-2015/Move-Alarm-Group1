<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nu.test4" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SetAlarm"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartOfficeActivity"
            android:label="@string/title_activity_start_office"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".EndOfficeActivity"
            android:label="@string/title_activity_end_office"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".SetPeriodActivity"
            android:label="@string/title_activity_set_period"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".WorkingdayActivity"
            android:label="@string/title_activity_workingday"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
